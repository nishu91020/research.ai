{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "LANGCHAIN_TRACING_V2": "false"
  },
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "api/health.py"
    },
    {
      "src": "/api/research/(.+)",
      "dest": "api/research.py"
    },
    {
      "src": "/api/?",
      "dest": "api/index.py"
    },
    {
      "handle": "filesystem"
    }
  ]
}
